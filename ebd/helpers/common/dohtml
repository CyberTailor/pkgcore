#!/usr/bin/env pkgcore-ebuild-helper
# Copyright: 2011 Brian Harring <ferringb@gmail.com>
# License: GPL2/BSD 3 clause

INSDESTTREE=usr/share/doc/${PF}/${PKGCORE_DOCDESTTREE}

# pass args back to python side to handle install
__ebd_write_line "dohtml numargs=2"
__ebd_write_line "dest=${INSDESTTREE}"
__IFS_push $'\a'
__ebd_write_line "args=$*"
__IFS_pop
__ebd_read_line result

__helper_exit "${result}"
