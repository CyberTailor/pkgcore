#!/usr/bin/env pkgcore-ebuild-helper
# Copyright: 2012 Brian Harring <ferringb@gmail.com>
# License: GPL2/BSD 3 clause

# prepend install options to arguments so both are parsed
args=( ${INSOPTIONS} "$@" )

# pass args back to python side to handle install
__ebd_write_line "doins numargs=2"
__ebd_write_line "dest=${INSDESTTREE}"
__IFS_push $'\a'
__ebd_write_line "args=${args[*]}"
__IFS_pop
__ebd_read_line result

__helper_exit "${result}"
